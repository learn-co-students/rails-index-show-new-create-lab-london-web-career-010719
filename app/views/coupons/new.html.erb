<h1>➕ Coupon Form ➕</h1>

<%= link_to "◀️ All Coupons", coupons_path %>

<%= form_tag coupons_path do %>

<%= label_tag 'store', 'Store:'%>
<%= text_field_tag :"coupon[store]", nil, id: 'store' %><br/>

<%= label_tag 'code', 'Coupon Code:'%>
<%= text_field_tag :"coupon[coupon_code]", nil, id: 'code' %><br/>

<%= submit_tag "Create Coupon" %>

<% end %>
